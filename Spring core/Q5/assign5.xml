<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">
           
           <context:annotation-config>
</context:annotation-config>
<bean id="studentRequired" class="springcore_example.Studnetq5">
        <property name="name" value="Rani"/>
        
    </bean>

    <bean id="stds" class="springcore_example.Studnetq5">
        <property name="name" value="Rani"/>
        <property name="age" value="10"/>
    </bean>
    <bean id="school" class = "springcore_example.Studnetq5">
        <property name="name" value="stdstotals"/>
