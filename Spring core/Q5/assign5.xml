    </bean>

    <bean id="school1" class = "springcore_example.Schoolq5">
        <property name="name" value="stdstotals"/>
        <property name="TotalStudent" value="100"/>

    </bean>
    <bean id="school2" class = "springcore_example.Schoolq5">
        <property name="name" value="stdtotal1"/>
        <property name="TotalStudent" value="300"/>

    </bean>

    <bean id="school3" class = "springcore_example.Schoolq5">
        <property name="name" value="stdtotal2"/>
