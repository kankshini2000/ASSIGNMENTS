<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
    <bean id="que" class="springcore_example.Collectiosnq2">
        <property name="quid"  value="1"/>
        <property name="que" value="what is ur name?"/>
        <property name="answers">
            <list>
                <value>My name is blah blah </value>
                <value>i am doing spring core assignments.</value>
                <value>yes plz give the correct output.</value>
            </list>
        </property>
    </bean>

    <bean id="que1" class="springcore_example.Collectiosnq2">
        <property name="quid"  value="1"/>
        <property name="que" value="what is ur name?"/>
        <property name="answersSet">
            <set>
                <value>My name is blah blah </value>
                <value>i am doing spring core assignments.</value>
                <value>yes plz give the correct output.</value>
            </set>
        </property>
    </bean>
                                    <!-- using set collection injection -->
    <bean id="que2" class="springcore_example.Collectiosnq2">
        <property name="quid"  value="1"/>
        <property name="que" value="what is ur name?"/>
        <property name="answersMap">
            <map>
                <entry key="1" value="My name is blah blah"/>
                <entry key="2" value="i am doing spring core assignments."/>
                <entry key="3" value="yes plz give the correct output."/>
            </map>
        </property>
    </bean>

</beans>